<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NFC_App.MainPage"
             xmlns:viewmodel="clr-namespace:NFC_App.ViewModel"
            xmlns:view="clr-namespace:NFC_App.View"
             x:DataType="viewmodel:LoginViewModel">

    <TabBar>
        <Tab Route="home"
             Title="home">
            <ShellContent ContentTemplate="{DataTemplate  view:Home}"/>
        </Tab>
        <Tab Route="test"
              Title="test">
            <ShellContent ContentTemplate="{DataTemplate  view:TestPage}"/>
        </Tab>
    </TabBar>

</Shell>
